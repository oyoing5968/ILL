public class TwoArray {
    public static void main(String[] args) {
        int n [][] = new int[5][];

        n[0] = new int[]{1, 2, 3};
        n[1] = new int[] {1, 2};
        n[2] = new int[] {1};
        n[3] = new int[] {1, 2, 3};
        n[4] = new int[] {1, 2, 3, 4};

        for (int i=0; i<n.length; i++){
            for (int j=0; j<n[i].length; j++){
                System.out.print(n[i][j]+" ");
            }
            System.out.println();
        }
    }
}
